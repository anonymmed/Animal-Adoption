{% extends 'TemplateBundle:Layout:frontpage.html.twig' %}


{% block content %}

    <div class="product">

        <div class="container">
            <div class="col-md-9">
                <div class="mid-popular">
                    {% for concour in concours  %}
                        <div class="col-md-4 item-grid1 simpleCart_shelfItem">

                            <div class=" mid-pop">
                                <div class="pro-img">

                                </div>
                                <div class="mid-1">
                                    <div class="pro-img">
                                        <img src="" class="img-responsive" alt="">

                                        <div class="zoom-icon ">
                                            <a class="picture" href="" rel="title" class="b-link-stripe b-animate-go  thickbox"><i class="glyphicon glyphicon-search icon "></i></a>
                                            <a href="{{ path('participer1',{'id':concour.id} )}}"><i class="glyphicon glyphicon-menu-right icon"></i></a>
                                        </div>
                                    </div>
                                    <div class="women">
                                        <img src="{{ asset('images/concour.jpg') }}"  class="img-responsive" >
                                        <h6> <span>{{  concour.description }}</span></h6>
                                        <h7> <span>{{concour.date | date }}</span></h7>
                                        <div class="women-top">
                                            <a href="{{ path('participer1',{'id':concour.id} )}}" class="btn btn-default"><span>Participer</span></a>
                                        </div>
                                        <div class="img item_add">
                                            <a href="{{ path('participer1',{'id':concour.id} )}}"><img src="{{ asset('images/ca.png') }}" alt=""></a>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="mid-2">
                                        <div class="block">
                                            <div class="starbox small ghosting"> </div>
                                        </div>

                                        <div class="clearfix"></div>
                                    </div>

                                </div>
                            </div>

                        </div>
                    {% endfor %}


                    <div class="clearfix"></div>
                </div>
            </div>


        </div>
    </div>
    <!--//content-->

{% endblock %}
